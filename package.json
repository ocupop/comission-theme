{
  "name": "comission-theme",
  "version": "1.0.0",
  "description": "Theme developed for CoMission Art",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "shopify theme dev --theme-editor-sync --environment comission",
    "dev": "yarn tw & yarn --cwd theme start",
    "scripts:watch": "rollup -c --watch",
